<div class="row">
  <div class="span4">
    <h2>WBS Nivel 2</h2>
  </div>
  <div class="span4">
    <br>
    <%= link_to raw('Nuevo <i class="icon-plus icon-white"></i>'), new_production_subsector_path, :class => "btn btn-primary pull-right" %>
  </div>
</div>
<br>

<div class="row">
<table class="table table-striped table-bordered table-condensed table-hover span8">
  <tr>
    <th>#id</th>
    <th>Codigo</th>
    <th>Descripcion</th>
    <th>Sectore</th>
    <th>Acciones</th>
  </tr>

<% @subsectors.each do |subsector| %>
  <tr>
    <td><%= subsector.id %></td>
    <td><%= subsector.codigo %></td>
    <td><%= subsector.descripcion %></td>
    <td><%= subsector.sector.descripcion %></td>
    <td>
      <%= link_to raw('<i class="icon-eye-open icon-white"></i>'), [:production,subsector], :class => "btn btn-small btn-info" %>
      <%= link_to raw('<i class="icon-edit icon-white"></i>'), edit_production_subsector_path(subsector), :class => "btn btn-small btn-success" %>
      <%= link_to raw('<i class="icon-trash icon-white"></i>'), [:production,subsector], method: :delete, data: { confirm: 'Esta Seguro que desea Eliminar?' }, :class => "btn btn-small btn-danger" %>
    </td>
  </tr>
<% end %>
</table>
</div>
<div class="clearfix"></div>