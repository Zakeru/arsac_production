<p id="notice"><%= notice %></p>
<div class="page-header">
  <h1>Ver Grupo</h1>
</div>
  
<div class="row">
<table class="table table-striped table-bordered table-condensed table-hover span6">
  <tr>
    <th>Nombre</th>
    <th>Descripcion</th>
  </tr>
  <tr>
    <td>#id</td>
    <td><%= @groupsofwork.id %></td>
  </tr>
  <tr>
    <td>WBS Nivel 1</td>
    <td><%= @groupsofwork.sector.descripcion %></td>
  </tr>
  <tr>
    <td>WBS Nivel 2</td>
    <td><%= @groupsofwork.subsector.descripcion %></td>
  </tr>
  <tr>
    <td>Jefe de Frente</td>
    <td>
      <%= @groupsofwork.chiefoffront.entity.nombre %>
      <%= @groupsofwork.chiefoffront.entity.apellido %>
      <%= @groupsofwork.chiefoffront.entity.razon_social %>
    </td>
  </tr>
  <tr>
    <td>Ejecutor</td>
    <td>
      <%= @groupsofwork.subcontract.supplier.entity.nombre %>
      <%= @groupsofwork.subcontract.supplier.entity.apellido %>
      <%= @groupsofwork.subcontract.supplier.entity.razon_social %>
    </td>
  </tr>
  <tr>
    <td>Maestro de Obra</td>
    <td>
      <%= @groupsofwork.teacherofwork.entity.nombre %>
      <%= @groupsofwork.teacherofwork.entity.apellido %>
      <%= @groupsofwork.teacherofwork.entity.razon_social %>
    </td>
  </tr>
  <tr>
    <td>Activo</td>
    <td><%= @groupsofwork.activo %></td>
  </tr>
</table>
</div>

<div class="form-actions">
  <%= link_to raw('<i class="icon-edit icon-white"></i> Editar'), edit_production_groupsofwork_path(@groupsofwork), :class => 'btn btn-success' %>
  <%= link_to raw('<i class="icon-arrow-left icon-white"></i> Regresar'), production_groupsofworks_path, :class => 'btn btn-primary' %>  
</div>

